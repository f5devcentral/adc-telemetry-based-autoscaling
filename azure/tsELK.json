{
    "class": "Telemetry",
    "controls": {
      "class": "Controls",
      "logLevel": "debug"
    },
    "My_Poller": {
      "class": "Telemetry_System_Poller",
      "interval": 60,
      "actions": [{         
         "includeData": {},
         "locations": {
             "avr": true  
         }    
       }]
    },
    "My_Listener": {
      "class": "Telemetry_Listener",
      "port": 6514
    },
     "My_ELK_Consumer": {
        "class": "Telemetry_Consumer",
        "type": "Generic_HTTP",
        "host": "${logStashIP}",
        "protocol": "http",
        "port": ${logStashPort},
        "path": "/",
        "method": "POST",
        "headers": [
             {
               "name": "content-type",
               "value": "application/json"
             }
         ]
     }
 }
